@keyframes blurData {
  0% {
      filter: blur(5px);
  }
  100% {
      filter: blur(5px);
  }
}

@keyframes unBlurData {
0% {
    filter: blur(0px);
}
100% {
    filter: blur(0px);
}
}



.sensitveData {
    animation: blurData 1s infinite;
}


.sensitveData:hover {
  animation: unBlurData 1s infinite;

}
